page{
  height: 100%;
}
.container_main{
  padding: 18rpx;
  height: 100%;
  background: #f1f1f1;
}

.header{
  padding: 0 20rpx;
  height: 70rpx;
  background: #fff;
}
.input{
  height: 72rpx;
}
.company_title{
  font-size: 26rpx;
  color: #808080;
  line-height: 70rpx;
  float: left
}

.container_content{
  margin-top: 20rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}
.container_left{
  width: 20%;
}
.container_right{
  width: 80%;
}
.s_title{
  width: 100%;
}

.area{
  width: 18%;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 24rpx;
  float: left;
  margin-left:1.1%;
  margin-top: 10rpx;
  color: #9b9b9b;
  border-radius: 10rpx;
  /*background-color: rgba(255, 125, 24, 0.1);*/
  border: 1rpx solid #9b9b9b;
}
.full{
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 26rpx;
  float: left;
  margin-right: 0.6%;
  margin-top: 10rpx;
  border-radius: 10rpx;
  color: #9b9b9b;
  background-color: #e3e3e3;
  border-color: transparent;
}
.quan{
  font-size:24rpx;
  line-height: 70rpx;
  color:#9b9b9b;
  float: right
}
.quan switch{
  zoom:0.6;
}
.active{
  background: #26c1ea !important;
  color: #fff !important;
  border-color:#26c1ea 
}
.save{
  width: 100%;
  margin-top: 100rpx;
  background: #26c1ea;
  border: #26c1ea;
  color: #fff;
  font-size: 30rpx;
}